<div id="properties-panel" data-spy="scroll" data-target=".properties-navbar" class="panel panel-primary properties-panel--scrollable">
  <div class="properties-panel--header panel-heading">
    <h4 class="panel-title">Properties <small>Scroll me</small></h4>
  </div>

  <div class="panel-body">
    <p>List of properties the video has.</p>

    <nav class="properties-navbar navbar">
      <div class="collapse navbar-collapse" id="propertiesNav">
        <ul class="nav navbar-nav">
          {{#each-in data.modelData.videos.[0] as |key value|}}
            <li data-toggle="tooltip" title={{get-display-name data.modelConfig.videos.data.video.data key}}><a href="#{{key}}"><span class="glyphicon glyphicon-picture"/></a></li>
          {{/each-in}}
          <li data-toggle="tooltip" title={{data.modelConfig.relations.displayName}}><a href="#relations"><span class="glyphicon glyphicon-picture"/></a></li>
        </ul>
      </div>
    </nav>

    {{generic-form noLabel=false data=data.modelConfig.videos.data.video}}
    <!--
    {{#each-in data.modelData.videos.[0] as |key value|}}
      <p id={{key}}><b>{{get-display-name data.modelConfig.videos.data.video.data key}}: </b>{{value}}</p>
    {{/each-in}}
    -->
  </div>

  <div class="properties-panel__footer panel-footer">
    <p>Submit at bottom of scroll</p>
  </div>
</div>
