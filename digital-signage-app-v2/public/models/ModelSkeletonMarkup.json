{
    "config": {
        "prettyName": "", 
        "modelIdentifier": "",
        "description" : "",
        "backgroundVideos": [ ], array of background videos by key to video object
        "ui": {
            "idle": 0, seconds system is idle before returning home
            "menuDwell": 0, seconds menu should remain after mouse leaves
            "menuLocale": "" top right bottom left
        },
        "keyboard": { keys for keyboard actions
            "select": "",
            "previous": "",
            "cancel": "",
            "next": ""
        }
    },
    "videos": {
        "0": {
            "prettyName": "", title
            "description": "", description of video
            "attributes": [ ], array of attributes by key to attribute object
            "full": {
                "fileIdentifier": "", the name of the video file
                "isUrl": false, true or false. false for local, true for links to other sites
                "attribution": "" attribution for the video
            },
            "teaser": { same as above
                "fileIdentifier": "",
                "isUrl": false,
                "attribution": ""
            }
        }
    },
    "attributes": {
        "0": {
            "prettyName": "",
            "description": "",
            "x": 0, x position on screen as decimal percent (e.g. 0.5, 0.75) from left
            "y": 0, x position on screen as decimal percent (e.g. 0.5, 0.75) from top
            "videos": [ ] videos associated with this attribute
        }
    },
    "relations": {
        "0": {
            "videoId": "0", start video
            "relatedId": "0", end video
            "difficulty": 0, how hard end video is relative from the start video
            "attributeId": "0" attribute by key to attribute object (why they are related)
        }
    }
}
