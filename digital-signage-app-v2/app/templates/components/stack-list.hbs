{{yield}}
{{#if isFlex}}
	<div class="stack-list--flex">
    {{#each-in data as |key value|}}
        <div class="{{stackItemClass}}">
          {{video-stack videos=value.videos borderStyle=(if (video-selected key selectedStackIndex) "blue-border") loop=true onClickCallback=(action 'stackClicked') onHoverCallback=(action 'stackHovered') selectedStackIndex=key}}
        </div>
    {{/each-in}}
	</div>
{{else}}
    {{#each-in data as |key value|}}
      <div style={{make-stack-style x=value.x y=value.y}}>
        {{video-stack videos=value.videos  highlightedStyle=(if (video-selected key selectedStackIndex) stackItemHighlight) isMuted=(if (video-selected key selectedStackIndex) isMuted false)loop=true onSelectedCallback=(action 'select') 
        onClickCallback=(action 'stackSelected') cancelCallback=(action 'cancel') onHoverCallback=(action 'hover') selectedStackIndex=key}}
      </div>
    {{/each-in}}
{{/if}}

